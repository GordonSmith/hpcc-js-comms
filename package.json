{
  "name": "hpcc-platform-comms",
  "version": "1.0.0",
  "description": "HPCC Platform Client Comms Library (TS/JS)",
  "keyword": [],
  "homepage": "",
  "license": "Apache-2.0",
  "author": "HPCC Systems",
  "main": "dist/hpcc-platform-comms.node.js",
  "browser": "dist/hpcc-platform-comms.js",
  "module": "dist-es6/index",
  "jsnext:main": "dist-es6/index.js",
  "scripts": {
    "transpile-es2015": "tsc -p ./tsconfig-es2015.json",
    "rollup-es2015": "npm run transpile-es2015 && rollup -c rollup.config.js",
    "rollup-node": "npm run transpile-es2015 && rollup -c rollup-node.config.js",
    "compile": "tsc -p .",
    "watch-typescript": "tsc -w -p .",
    "watch-typescript-rollup": "tsc -w -p ./tsconfig-rollup.json",
    "webpack": "webpack",
    "compile-rollup": "tsc -p ./tsconfig-rollup.json",
    "rollup": "rollup -c",
    "watch-rollup": "rollup -w -c",
    "build-webpack": "npm run compile && npm run webpack",
    "build-rollup": "npm run compile-rollup && npm run rollup",
    "test": "mocha lib/test/",
    "watch-all": "npm run watch-typescript"
  },
  "dependencies": {
    "es6-promise": "~4.0.5",
    "d3-request": "~1.0.3"
  },
  "devDependencies": {
    "@types/chai": "~3.4.34",
    "@types/d3-request": "~1.0.1",
    "@types/mocha": "~2.2.37",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "chai": "~3.5.0",
    "mocha": "~3.2.0",
    "requirejs": "~2.3.2",
    "rollup": "~0.41.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-includepaths": "~0.1.8",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "source-map-loader": "^0.1.5",
    "typescript": "^2.1.4",
    "webpack": "2.2.0-rc.3",
    "webpack-uglify-js-plugin": "~1.1.9",
    "xmlhttprequest": "~1.8.0"
  }
}