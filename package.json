{
  "name": "hpcc-platform-comms",
  "version": "1.0.0",
  "description": "HPCC Platform Client Comms Library (TS/JS)",
  "keyword": [],
  "homepage": "",
  "license": "Apache-2.0",
  "author": "HPCC Systems",
  "browser": "dist/hpcc-platform-comms.min.js",
  "main": "dist/commonjs/index",
  "module": "dist/es6/index",
  "jsnext:main": "dist/es6/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "tsc-es2015": "tsc -p .",
    "tsc-commonjs": "tsc -p ./tsconfig-commonjs.json",
    "tsc-test": "tsc -p ./test",
    "webpack": "webpack",
    "webpack-min": "cross-env NODE_ENV=min webpack --display-modules",
    "webpack-min-stats": "cross-env NODE_ENV=min webpack --json > stats.json",
    "webpack-min-debug": "cross-env NODE_ENV=min-debug webpack",
    "build": "npm run tsc-es2015 && npm run tsc-commonjs && npm run webpack && npm run webpack-min",
    "watch": "cross-env NODE_ENV=watch webpack",
    "lint": "tslint ./src/**/*",
    "test": "npm run lint && npm run tsc-test && mocha ./dist-test/test/index.js",
    "prepublish": "npm run build",
    "doc": "typedoc --out ./docx/ --tsconfig ./tsconfig.json --exclude node_modules --excludeNotExported --ignoreCompilerErrors src"
  },
  "dependencies": {
    "es6-promise": "~4.0.5",
    "hash-sum": "~1.0.2",
    "tslib": "~1.5.0",
    "xhr": "~2.3.3"
  },
  "devDependencies": {
    "@types/chai": "~3.4.34",
    "@types/mocha": "~2.2.39",
    "awesome-typescript-loader": "~3.0.0-beta.18",
    "cross-env": "~3.1.4",
    "chai": "~3.5.0",
    "mocha": "~3.2.0",
    "requirejs": "~2.3.2",
    "tslint": "~4.4.2",
    "typedoc": "~0.5.5",
    "typescript": "~2.1.5",
    "webpack": "^2.2.1",
    "webpack-node-externals": "~1.5.4",
    "xmldom": "~0.1.27",
    "request": "~2.79.0"
  }
}